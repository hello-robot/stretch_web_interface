{
    "name": "hello-robot-server",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "rm -rf dist && npm run copy && webpack --mode development;",
        "copy": "npm run copy_boostrap && npm run copy_static",
        "copy_boostrap": "mkdir -p dist/shared && cp node_modules/bootstrap/dist/css/bootstrap.css node_modules/bootstrap/dist/js/bootstrap.js dist",
        "copy_static": "rsync -va --prune-empty-dirs --include '*/' --include '*.css' --include '*/images/*' --exclude '*' src/pages/ src/style.css dist"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.2",
        "@types/roslib": "^1.1.8",
        "body-parser": "^1.19.0",
        "bootstrap": "^5.1.3",
        "connect-redis": "^6.0.0",
        "cookie-parser": "^1.4.5",
        "debug": "^4.3.1",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "firebase": "^9.6.9",
        "helmet": "^4.6.0",
        "html-webpack-plugin": "^5.5.0",
        "mongoose": "^5.12.14",
        "morgan": "^1.10.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.1.0",
        "passport.socketio": "^3.7.0",
        "postprocessing": "^6.25.0",
        "pug": "^3.0.2",
        "puppeteer": "^13.3.1",
        "redis": "^3.1.2",
        "roslib": "git@github.com:RobotWebTools/roslibjs.git#develop",
        "serve-favicon": "^2.5.0",
        "socket.io": "^4.4.1",
        "socket.io-client": "^4.4.1",
        "three": "^0.138.3"
    },
    "devDependencies": {
        "@types/bootstrap": "^5.1.9",
        "@types/three": "^0.138.0",
        "nodemon": "^2.0.15",
        "snowpack": "^3.8.8",
        "ts-loader": "^9.2.8",
        "tsc-silent": "^1.2.2",
        "typescript": "^4.6.2",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2"
    }
}
